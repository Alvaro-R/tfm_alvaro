@article{wenqianCrystalStructureZika2017,
  title = {The Crystal Structure of {{Zika}} Virus {{NS5}} Reveals Conserved Drug Targets},
  author = {Wenqian, Duan and Song, Hao and Wang, Haiyuan and Chai, Yang and Su, Chao and Qi, Jianxun and Shi, Yi and Gao, George F.},
  year = {2017},
  month = apr,
  journal = {The EMBO Journal},
