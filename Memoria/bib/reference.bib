@article{leiCrystalStructureZika2016,
  title = {Crystal Structure of {{Zika}} Virus {{NS2B-NS3}} Protease in Complex with a Boronate Inhibitor},
  author = {Lei, Jian and Hansen, Guido and Nitsche, Christoph and Klein, Christian D. and Zhang, Linlin and Hilgenfeld, Rolf},
