@article{gulloCongenitalZikaSyndrome2022,
  title = {Congenital {{Zika Syndrome}}: {{Genetic Avenues}} for {{Diagnosis}} and {{Therapy}}, {{Possible Management}} and {{Long-Term Outcomes}}},
  shorttitle = {Congenital {{Zika Syndrome}}},
  author = {Gullo, Giuseppe and Scaglione, Marco and Cucinella, Gaspare and Riva, Arianna and Coldebella, Davide and Cavaliere, Anna Franca and Signore, Fabrizio and Buzzaccarini, Giovanni and Spagnol, Giulia and Lagan{\`a}, Antonio Simone and Noventa, Marco and Zaami, Simona},
