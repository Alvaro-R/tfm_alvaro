@article{zhangCrystalStructureUnlinked2016,
  title = {Crystal Structure of Unlinked {{NS2B-NS3}} Protease from {{Zika}} Virus},
  author = {Zhang, Zhenzhen and Li, Yan and Loh, Ying Ru and Phoo, Wint Wint and Hung, Alvin W. and Kang, CongBao and Luo, Dahai},
